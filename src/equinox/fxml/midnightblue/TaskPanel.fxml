<?xml version="1.0" encoding="UTF-8"?>

<?import org.controlsfx.control.*?>
<?import javafx.scene.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox fx:id="root_" spacing="5.0" styleClass="taskPanel" stylesheets="@../../css/midnightblue/Commons.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="equinox.controller.TaskPanel">
<padding>
<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
</padding>
<children>
<ProgressIndicator fx:id="progressIndicator_" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="68.0" prefWidth="60.0" progress="0.26" HBox.hgrow="NEVER" /><VBox maxWidth="1.7976931348623157E308" spacing="5.0" HBox.hgrow="ALWAYS">
<children>
<Label fx:id="title_" maxWidth="1.7976931348623157E308" text="Task Title..." VBox.vgrow="NEVER">
<font>
<Font name="System Bold" size="13.0" />
</font>
</Label>
<Label fx:id="message_" layoutX="10.0" layoutY="10.0" maxWidth="1.7976931348623157E308" text="Progress info..." textFill="GREY" VBox.vgrow="NEVER">
<font>
<Font size="10.0" />
</font>
</Label>
<Separator maxWidth="1.7976931348623157E308" VBox.vgrow="NEVER" />
<HBox alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308" spacing="5.0" VBox.vgrow="NEVER">
<children>
                  <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" spacing="5.0" HBox.hgrow="ALWAYS">
                     <children>
                        <ToggleSwitch fx:id="notify_" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="35.0" HBox.hgrow="NEVER" />
                        <Label maxWidth="1.7976931348623157E308" text="Notify when task finished" HBox.hgrow="ALWAYS">
                           <font>
                              <Font size="10.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
<Button fx:id="cancel_" layoutX="203.0" layoutY="10.0" mnemonicParsing="false" onAction="#onCancelClicked" styleClass="menuButton" text="Cancel" HBox.hgrow="NEVER">
<font>
<Font size="10.0" />
</font>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
</Button>
</children>
</HBox>
</children>
</VBox>
</children></HBox>
