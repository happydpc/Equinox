<?xml version="1.0" encoding="UTF-8"?><!--Root input element. This element is obligatory.-->
<equinoxInput version="" title="" description="" author="" date="" time="">
	
	<!--Instruction settings. This element and all its children are optional. If not supplied, default settings will be used at execution time.-->
	<settings>
		<!--Run mode can be one of 'parallel' (default), 'sequential' or 'save'. Note that, save mode will only save first level tasks (i.e. not the dependent tasks).-->
		<runMode>parallel</runMode><!--If runSilent is set to true, all notifications (except for errors and warnings) will be suppressed.-->
		<runSilent>false</runSilent><!--Analysis engine settings are same as seen in Data Analyst user interface. The settings 'engine', 'version' and 'subVersion' are case sensitive and the rest are boolean (i.e. true or false). -->
		<analysisEngine><!--Engine can be one of 'ISAMI Engine', 'SAFE Engine' or 'Inbuilt Engine'.-->
			<engine>Inbuilt Engine</engine>
			<version>v9.7.0</version>
			<subVersion>Derivatives</subVersion>
			<keepOutputFile>true</keepOutputFile>
			<performDetailedAnalysis>true</performDetailedAnalysis>
			<applyCompressionForPropagation>true</applyCompressionForPropagation>
			<fallbackToInbuilt>true</fallbackToInbuilt>
		</analysisEngine>
	</settings>
	
	
	<!--Adds spectrum to workspace from supplied CDF set files. TXT file is optional, if not given, will be generated from CVT and XLS.-->
	<addSpectrum>
		<id></id>
		<anaPath></anaPath>
		<cvtPath></cvtPath>
		<flsPath></flsPath>
		<xlsPath></xlsPath>
		<txtPath></txtPath>
		<convSheet></convSheet>
	</addSpectrum>
	
	<!--Adds spectrum to workspace from suplied SPEC bundle.-->
	<addSpectrum>
		<id></id>
		<specPath></specPath>
	</addSpectrum>
	
	<!--Adds STF file to specified spectrum.-->
	<addStf>
		<id></id><spectrumId></spectrumId><stfPath></stfPath>
	</addStf>
	
	<!--Adds stress sequence to workspace from given SIGMA file.-->
	<addStressSequence>
		<id></id>
		<sigmaPath></sigmaPath>
	</addStressSequence>
	
	<!--Adds stress sequence to workspace from given STH and FLS files.-->
	<addStressSequence>
		<id></id>
		
		<sthPath></sthPath>
		<flsPath></flsPath>
	</addStressSequence>
	
	<!--Generates stress sequence from given STF file.-->
	<generateStressSequence>
		<id></id>
		<stfId></stfId>
		<inputPath></inputPath>
	</generateStressSequence>
	
	<saveMissionProfilePlot>
		<id></id><stressSequenceId></stressSequenceId>
		<outputPath></outputPath></saveMissionProfilePlot>
	<saveTypicalFlightPlot>
		<id></id>
		<stressSequenceId></stressSequenceId><typicalFlightName></typicalFlightName>
		<outputPath></outputPath></saveTypicalFlightPlot>
	
	<equivalentStressAnalysis>
		<id></id>
		<stfId></stfId>
		<inputPath></inputPath>
	</equivalentStressAnalysis>
	
	<equivalentStressAnalysis>
		<id></id>
		<stressSequenceId></stressSequenceId>
		<inputPath></inputPath>
	</equivalentStressAnalysis>
	<saveLevelCrossingPlot>
		<id></id>
		<equivalentStressId></equivalentStressId>
		<outputPath></outputPath></saveLevelCrossingPlot>
	<saveAnalysisOutputFile>
		<id></id>
		<outputPath></outputPath></saveAnalysisOutputFile></equinoxInput>